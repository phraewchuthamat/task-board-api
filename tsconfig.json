{
    "compilerOptions": {
        "target": "es2020",
        "module": "Node16",
        "rootDir": "./src",
        "moduleResolution": "node16",
        "outDir": "./dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": ["src/**/*"]
}
